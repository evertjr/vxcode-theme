      ],
      "settings": {
        "foreground": "#DABAFF"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF7AB2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSX Components",
