    // Base Colors
    "focusBorder": "#26966a",
    "foreground": "#515151",
    "disabledForeground": "#999999",
    "widget.shadow": "#00000019",
    "selection.background": "#26966a4f",
    "sash.hoverBorder": "#e38181",
    // Text colors
    "textCodeBlock.background": "#e9e9e9",
    "textLink.activeForeground": "#af5d5d",
    "textLink.foreground": "#af5d5dcc",
    // Action colors
    "toolbar.hoverBackground": "#00000010",
    // Button control
    "button.background": "#af5d5dcf",
    "button.hoverBackground": "#af5d5d",
    // Dropdown control
    "dropdown.background": "#f9f9fb",
